{
  "animesaturn": {
    "base_url": "https://www.animesaturn.fun",
    "search_url": "https://www.animesaturn.fun/search",
    "selectors": {
      "search_results": ".serie-box",
      "anime_title": ".serie-box .serie-title",
      "anime_link": ".serie-box a",
      "episode_list": ".episode-list .episode",
      "episode_link": ".episode a",
      "video_player": ".video-player iframe",
      "video_sources": ".video-sources source"
    },
    "headers": {
      "User-Agent": "Mozilla/5.0 (Windows NT 10.0; Win64; x64) AppleWebKit/537.36 (KHTML, like Gecko) Chrome/91.0.4472.124 Safari/537.36",
      "Accept": "text/html,application/xhtml+xml,application/xml;q=0.9,image/webp,*/*;q=0.8",
      "Accept-Language": "it-IT,it;q=0.8,en-US;q=0.5,en;q=0.3",
      "Accept-Encoding": "gzip, deflate"
    }
  },
  "animeunity": {
    "base_url": "https://www.animeunity.tv",
    "api_url": "https://www.animeunity.tv/api/anime",
    "search_url": "https://www.animeunity.tv/api/search",
    "selectors": {
      "search_results": ".anime-card",
      "anime_title": ".anime-title",
      "anime_link": ".anime-card a",
      "episode_list": ".episode-item",
      "episode_link": ".episode-item a",
      "video_player": "#player",
      "video_sources": "video source"
    },
    "headers": {
      "User-Agent": "Mozilla/5.0 (Windows NT 10.0; Win64; x64) AppleWebKit/537.36 (KHTML, like Gecko) Chrome/91.0.4472.124 Safari/537.36",
      "Accept": "application/json, text/javascript, */*; q=0.01",
      "Content-Type": "application/json",
      "X-Requested-With": "XMLHttpRequest"
    }
  },
  "gogoanime": {
    "base_url": "https://gogoanime.lu",
    "search_url": "https://gogoanime.lu/search.html",
    "ajax_url": "https://ajax.gogocdn.net/ajax",
    "selectors": {
      "search_results": ".items li",
      "anime_title": ".items li .name a",
      "anime_link": ".items li .name a",
      "episode_list": "#episode_page li a",
      "episode_link": "a",
      "video_player": ".play-video",
      "video_sources": ".anime_muti_link ul li a"
    },
    "headers": {
      "User-Agent": "Mozilla/5.0 (Windows NT 10.0; Win64; x64) AppleWebKit/537.36 (KHTML, like Gecko) Chrome/91.0.4472.124 Safari/537.36",
      "Accept": "text/html,application/xhtml+xml,application/xml;q=0.9,image/webp,*/*;q=0.8",
      "Accept-Language": "en-US,en;q=0.5",
      "Accept-Encoding": "gzip, deflate"
    }
  }
}
